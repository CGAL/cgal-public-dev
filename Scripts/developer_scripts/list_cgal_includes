TMPFILE="/tmp/cgal_find_includes"
rm -f ${TMPFILE}
touch ${TMPFILE}
find include/CGAL src -type f -exec grep '^ *# *include *<' {} \; >> ${TMPFILE}
find include/CGAL src -type f -exec grep '^ *# *include *"' {} \; >> ${TMPFILE}
grep -v 'CGAL/' ${TMPFILE}|grep -v 'LEDA/' |tr -d " "|sort|uniq
rm -f ${TMPFILE}

